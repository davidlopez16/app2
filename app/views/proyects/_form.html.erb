<div class="right_col" role="main">
    <div class="">

<%= form_for(@proyect) do |f| %>
  <% @proyect.errors.full_messages.each do |message| %>
    <%= message %>
    
  <% end %>

  <div class="field">
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </div>
  <div class="field">
    <%= f.label :description %><br>
    <%= f.text_area :description, class: "form-control", rows:"3" %>
  </div>
  <div class="field">
    <%= f.label :image %><br>
    <%= f.file_field :image %>
  </div>
  <div class="field">
    <%= f.label :file %><br>
    <%= f.file_field :file %>
  </div>

  <div class="actions">
<%= f.submit class: "btn btn-success"%>
  </div>

<% end %>
</div>
</div>
