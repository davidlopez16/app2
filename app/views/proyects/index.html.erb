<div class="right_col" role="main">
   
<h1>Listing proyects</h1>
  <tbody>
  <div class="col-md-55">
  <div class="thumbnail">
   <div class="image view view-first">
       <% @proyects.each do |proyect| %>
       <%= proyect.image, class:"", width:"100%", height:"100%" %>
        <div class="mask">
        <td><%= proyect.title %></td>
        <div class="tools tools-bottom">
        <%= link_to 'Show', proyect, class:"fa fa-link" %>
        <%= link_to 'Edit', edit_proyect_path(proyect) %>
        <%= link_to 'Destroy', proyect, method: :delete, data: { confirm: 'Are you sure?' } %>
         </div>
         </div>
         <div class="caption">
       <%= proyect.description %>
        <%= proyect.file %> 
      </div>
    <% end %>
</div>
</div>
</div>



  </tbody>


<br>

           
</div>

<%= link_to 'New Proyect', new_proyect_path %>
